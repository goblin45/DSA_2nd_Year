import java.util.*;

class Node
{
    int data; Node next;
    Scanner sc = new Scanner (System.in);
    Node ()
    {
	System.out.println ("Enter data: ");
	num = sc.nextInt ();
        this.data = num;
        this.next = null; 
    }
}

class LinkedList extends Node 
{
    Node head; int num;
    Scanner sc = new Scanner (System.in);
    LinkedList ()
    {
        System.out.println ("Enter first data: ");
        Node start = new Node ();
    }
    void display ()
    {
        Node ptr = start;
        while (ptr != null)
        {
            System.out.println (ptr.data + "\t");
            ptr = ptr.next;
        }
    }
    void insert_at_beg ()
    {
	Node new_node = new Node ();
	new_node.next = start;
	
    }
    void insert_at_end ()
    {
        Node ptr = start;
        Node new_node = new Node ();
	while (ptr.next! = null)
	    ptr = ptr.next;
	ptr.next = new_node
    }
    void insert_at_any ()
    {
        
    }
    void delete_at_beg ()
    {
        
    }
    void delete_at_end ()
    {
        
    }
    void delete_at_any ()
    {
        
    }
}

class Main extends LinkedList
{
    public static void main (String args [])
    {
        LinkedList list1 = new LinkedList ();
        list1.insert_at_beg ();
	list1.display ();
    }
}
